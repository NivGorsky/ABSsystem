<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>

<TabPane fx:id="tabPane" prefHeight="600.0" prefWidth="950.0" styleClass="root" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="customer.CustomerController">
 <tabs>
   <Tab fx:id="informationTab" styleClass="root" text="Information">
         <content>
            <fx:include fx:id="information" source="information/Information.fxml" />
         </content>
      </Tab>
   <Tab fx:id="scrambleTab" styleClass="root" text="Scramble">
        <content>
           <fx:include fx:id="scramble" source="scramble/Scramble.fxml" />
        </content>
      </Tab>
     <Tab fx:id="paymentTab" styleClass="root" text="Payment">
        <content>
           <fx:include fx:id="payment" source="payment/Payment.fxml" />
        </content>
      </Tab>
 </tabs>
   <stylesheets>
      <URL value="@../css/DarkMode.css" />
      <URL value="@../css/LightMode.css" />
   </stylesheets>
</TabPane>
